<%-include('partials/header')%>

<div class="row-parent container-fluid compose">
    <div class="post-parent">
        <h1>Compose</h1>
        <form action="" method="post">
            <div class="form-group">
                <label>Title</label>
                <input type="text" class="form-control" name="postTitle" placeholder="">
            </div>
            <div class="form-group">
                <label>Content</label>
                <textarea class="form-control " name="postContent" id="" cols="30" rows="10"></textarea>
            </div>
            <div class="form-group">
                <button class="btn publish " type="submit">Publish</button>
            </div>
        </form>
    </div>
</div>
<%- include('partials/footer')%>